<%= form_for @bookmark do |f| %>
<div class="form-group">
  <%= f.text_field :url, class: "form-control", placeholder: 'Type the url you want to bookmark' %>
</div>

<div class="form-group">
  <%= f.select :topic_id, @topics.all.collect {|t| [ t.id ] }, { prompt: "Select a topic for your URL" }, class: "form-control" %>
</div>


<div class="form-group">
  <%= f.submit "Submit", class: 'btn btn-success' %>
  </div>
<% end %>
